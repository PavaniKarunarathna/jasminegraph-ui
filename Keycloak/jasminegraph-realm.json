{
    "id": "jasminegraph",
    "realm": "jasminegraph",
    "enabled": true,
    "clients": [
        {
            "clientId": "jasminegraph-api",
            "enabled": true,
            "protocol": "openid-connect",
            "publicClient": true,
            "redirectUris": [
                "*"
            ],
            "directAccessGrantsEnabled": true,
            "protocolMappers": [
                {
                    "name": "audience",
                    "protocol": "openid-connect",
                    "protocolMapper": "oidc-audience-mapper",
                    "consentRequired": false,
                    "config": {
                        "included.client.audience": "jasminegraph-api",
                        "id.token.claim": "true",
                        "access.token.claim": "true"
                    }
                }
            ]
        }
    ]
}
